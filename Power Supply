@name Power Supply
@inputs PowerOn PowerCut ShutDown CPUUsage GPUUsage CRUsage SCUsage
@outputs Power 
@persist Cap PowerOnVal
@model models/bull/gates/resistor.mdl
if(first()){
    Cap=750
    PowerOnVal=0
}
interval(100)
if(changed(PowerOn)&PowerOn){
    PowerOnVal=!PowerOnVal
}
if(PowerOnVal){
    if(ShutDown){
        PowerOnVal=!PowerOnVal
    }
Power=(Cap-CPUUsage-GPUUsage-CRUsage-SCUsage)*PowerCut
}else{
Power=0
}
